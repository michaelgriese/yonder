{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/images/*.{png,jpg,gif,svg}", "/css/*", "/js/*", "/*.{js,css,map,ico,png,jpg,gif,svg}"]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "application/javascript",
        ".css": "text/css"
    },
    "globalHeaders": {
        "cache-control": "public, max-age=31536000, immutable"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    }
}